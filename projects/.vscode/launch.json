{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Rust: Debug Run",
      "type": "lldb",
      "request": "launch",
      "cargo": {
        "args": [
          "build"
        ],
        "filter": {
          "name": "projects",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "sourceLanguages": ["rust"],
      "initCommands": [
        "settings set target.process.thread.step-avoid-regexp ^<?(std|core|alloc)::"
      ]
    }
  ]
}
